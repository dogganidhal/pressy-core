{
  "name": "pressy-core",
  "version": "0.0.1",
  "description": "Cœur du système pressy",
  "main": "index.ts",
  "repository": {
    "url": "https://github.com/dogganidhal/pressy-core"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "set NODE_ENV=development && ./node_modules/nodemon/bin/nodemon.js",
    "prod": "set NODE_ENV=production ts-node src/index.ts",
    "sync": "node_modules/typeorm/cli.js schema:sync",
    "debug": "node --inspect-brk=3333 ./node_modules/ts-node/dist/bin.js src/index.ts && ./node_modules/nodemon/bin/nodemon.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "author": "Nidhal DOGGA <dogga.nidhal@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/dateformat": "^1.0.1",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.2",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/method-override": "0.0.31",
    "@types/node": "^10.9.4",
    "@types/node-fetch": "^2.1.2",
    "@types/pg": "^7.4.11",
    "@types/pg-connection-string": "^2.0.0",
    "@types/superagent": "^3.8.4",
    "@types/supertest": "^2.0.6",
    "@types/uuid": "^3.4.4",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "crypto": "^1.0.1",
    "dateformat": "^3.0.3",
    "debug": "^4.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "json2typescript": "^1.0.6",
    "jsonwebtoken": "^8.3.0",
    "le_node": "^1.7.1",
    "method-override": "^3.0.0",
    "moxios": "^0.4.0",
    "node-fetch": "^2.2.0",
    "nodemon": "^1.18.4",
    "pg": "^7.4.3",
    "pg-connection-string": "^2.0.0",
    "reflect-metadata": "^0.1.12",
    "superagent": "^4.0.0-beta.5",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "typed-rest-client": "^1.0.9",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.7",
    "typescript": "^3.0.3",
    "typescript-rest": "^1.7.0",
    "uuid": "^3.3.2",
    "uuid-token-generator": "^1.0.0"
  },
  "devDependencies": {
    "@types/moxios": "^0.4.8",
    "@types/randomstring": "^1.1.6",
    "jest": "^23.6.0",
    "randomstring": "^1.1.5",
    "supertest": "^3.3.0",
    "supertest-as-promised": "^4.0.2"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "tests/*",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jest.js"
  }
}
